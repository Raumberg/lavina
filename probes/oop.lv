class Player:
    void fn __init__(string name):
        this.name = name
        this.health = 100
        print("Player " + name + " created!")

    void fn heal(int amount):
        this.health = this.health + amount
        print(this.name + " healed by " + amount + ". Health: " + this.health)

auto p = Player("Alina")
p.heal(20)
print("Final health: " + p.health)
