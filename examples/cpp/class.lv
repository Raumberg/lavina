class Player:
    void fn __init__(string name):
        this.name = name
        this.health = 100

    void fn heal(int amount):
        this.health = this.health + amount

    void fn info():
        print("Player: " + this.name + ", HP: " + this.health)

auto p = Player("Alina")
p.info()
p.heal(20)
p.info()
