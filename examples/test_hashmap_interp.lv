void fn main():
    // Test string interpolation
    string name = "Alice"
    int age = 30
    print("Hello ${name}, you are ${age} years old")
    print("1 + 2 = ${1 + 2}")

    // Test hashmap basics
    hashmap[string, int] ages = []
    print("Empty: ${ages.is_empty()}")
    print("Size: ${ages.len()}")

    // Test vector with interpolation
    vector[string] items = ["a", "b", "c"]
    print("Items: ${items.len()}, first: ${items[0]}")
